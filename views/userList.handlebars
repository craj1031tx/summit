<table class="table">
  <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">userLevel</th>
        <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}  
    <tr>
        <td>{{name}}</td>
        <td>{{email}}</td>
        <td>{{userLevel}}</td>
        <td>
            <a class="btn btn-primary" href="/users/lol/{{id}}" role="button">Primary</a>
            <a class="btn btn-success" href="/users/lol/{{id}}" role="button">Success</a>
            <a class="btn btn-danger" href="/users/lol/{{id}}" role="button">Delete</a>
        <td>
    </tr>
    {{/each}}
  </tbody>
</table>