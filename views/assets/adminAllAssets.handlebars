<br>
<row>
    <h1>All Assets</h1>
</row>
<br>
<table class="table">
  <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Content Type</th>
        <th scope="col">privLevel</th>
        <th scope="col">Download</th>
        <th scope="col">View</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody>
    {{#each assets}}  
    <tr>
        <td>{{name}}</td>
        <td>{{contentType}}</td>
        <td>{{privLevel}}</td>
        <td>
            <a class="btn btn-success" href="/uploads/{{assetMulterName}}" download="{{assetOriginalName}}">Download</a>
        </td>
        <td>
            <a class="btn btn-info" href="/uploads/{{assetMulterName}}" target="_blank">View</a>
        </td>
        <td>
            <a class="btn btn-warning" href="/assets/edit/{{id}}">Edit</a>
        </td>
        <td>
            <a class="btn btn-danger">Delete</a>
        </td>               
    </tr>
    {{/each}}
  </tbody>
</table>
